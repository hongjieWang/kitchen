<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kitchen.system.mapper.KeyConfigMapper">

    <resultMap type="KeyConfig" id="KeyConfigResult">
        <result property="id"    column="id"    />
        <result property="key"    column="key"    />
        <result property="name"    column="name"    />
        <result property="appName"    column="app_name"    />
        <result property="status"    column="status"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKeyConfigVo">
        select id, key, name, app_name, status, del_flag, create_by, create_time, update_by, update_time from key_config
    </sql>

    <select id="selectKeyConfigList" parameterType="KeyConfig" resultMap="KeyConfigResult">
        <include refid="selectKeyConfigVo"/>
        <where>
            <if test="key != null  and key != ''"> and key = #{key}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="appName != null  and appName != ''"> and app_name like concat('%', #{appName}, '%')</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>

    <select id="selectKeyConfigById" parameterType="Long" resultMap="KeyConfigResult">
        <include refid="selectKeyConfigVo"/>
        where id = #{id}
    </select>

    <insert id="insertKeyConfig" parameterType="KeyConfig">
        insert into key_config
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="key != null and key != ''">key,</if>
            <if test="name != null and name != ''">name,</if>
            <if test="appName != null">app_name,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="key != null and key != ''">#{key},</if>
            <if test="name != null and name != ''">#{name},</if>
            <if test="appName != null">#{appName},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>

    <update id="updateKeyConfig" parameterType="KeyConfig">
        update key_config
        <trim prefix="SET" suffixOverrides=",">
            <if test="key != null and key != ''">key = #{key},</if>
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="appName != null">app_name = #{appName},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKeyConfigById" parameterType="Long">
        delete from key_config where id = #{id}
    </delete>

    <delete id="deleteKeyConfigByIds" parameterType="String">
        delete from key_config where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>